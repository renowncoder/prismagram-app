{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "linebreak-style": [0],
        "arrow-parens": ["error", "as-needed"],
        "react/jsx-props-no-spreading": [0],
        "comma-dangle": ["error", "never"],
        "react/prop-types": [0],
        "no-else-return": [0],
        "consistent-return": [0]
    }
}